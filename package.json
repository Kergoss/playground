{
    "name": "schule",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "npm run build:frontend && npm run build:backend && node backend/be-dist/index.js",
        "start:frontend": "ng serve --proxy-config proxy.config.json",
        "start:backend": "node backend/be-dist/backend/src/index.js",
        "build:frontend": "ng build",
        "build:backend": "tsc -p backend/tsconfig.json",
        "build:backend:watch": "tsc -p backend/tsconfig.json --watch",
        "watch": "ng build --watch --configuration development",
        "test": "ng test"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^15.0.0",
        "@angular/cdk": "^15.0.0",
        "@angular/common": "^15.0.0",
        "@angular/compiler": "^15.0.0",
        "@angular/core": "^15.0.0",
        "@angular/forms": "^15.0.0",
        "@angular/material": "^15.0.0",
        "@angular/platform-browser": "^15.0.0",
        "@angular/platform-browser-dynamic": "^15.0.0",
        "@angular/router": "^15.0.0",
        "body-parser": "^1.20.1",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "fs-extra": "^10.1.0",
        "rxjs": "~7.5.0",
        "sqlite3": "^5.1.2",
        "tslib": "^2.3.0",
        "zone.js": "~0.12.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^15.0.0",
        "@angular/cli": "~15.0.0",
        "@angular/compiler-cli": "^15.0.0",
        "@types/fs-extra": "^9.0.13",
        "@types/jasmine": "~4.3.0",
        "@types/node": "^18.11.9",
        "@types/sqlite3": "^3.1.8",
        "jasmine-core": "~4.5.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.0.0",
        "prettier": "^2.7.1",
        "typescript": "~4.8.2"
    }
}
